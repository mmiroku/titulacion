<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
    version="2.1">

    <navigation-rule>
        <navigation-case>
            <from-outcome>usuarioInit</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>alumnos</from-outcome>
            <to-view-id>/secured/titulacion/alumnos.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>maestros</from-outcome>
            <to-view-id>/secured/titulacion/maestros.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>materias</from-outcome>
            <to-view-id>/secured/titulacion/materias.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>seminarios</from-outcome>
            <to-view-id>/secured/titulacion/seminarios.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>usuarios</from-outcome>
            <to-view-id>/secured/titulacion/usuarios.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cedulas</from-outcome>
            <to-view-id>/secured/titulacion/cedulas.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>catSeminarios</from-outcome>
            <to-view-id>/secured/titulacion/catSeminarios.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>catLineas</from-outcome>
            <to-view-id>/secured/titulacion/catLineas.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>expedientes</from-outcome>
            <to-view-id>/secured/titulacion/expedientes.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>alumnoLineas</from-outcome>
            <to-view-id>/secured/titulacion/alumnoLinea.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>login.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <application>
        <resource-bundle>
            <base-name>mx.upiicsa.titulacion.util.messages</base-name>
            <var>msg</var>
        </resource-bundle>
        <message-bundle>mx.upiicsa.titulacion.util.messages</message-bundle>
        <locale-config>
            <default-locale>es</default-locale>            
        </locale-config>
    </application>
</faces-config>
